{
    "domains": [
        {
            "name": "免费节点合集",
            "enable": true,
            "liveness": true,
            "sub": [
                "https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/Proxies.ini",
                "https://raw.githubusercontent.com/maicss-max/Clash-Rules/main/Long/Proxies.ini",
                "https://raw.githubusercontent.com/Pawdroid/Free-servers/main/sub",
                "https://raw.githubusercontent.com/learnhard-cn/free_proxy_ss/main/clash",
                "https://raw.githubusercontent.com/mfuu/v2ray/master/v2ray",
                "https://raw.githubusercontent.com/ermaozi/get_subscribe/main/subscribe/clash.yml",
                "https://raw.githubusercontent.com/pojiezhiyuan/jdvmess/main/jd",
                "https://raw.githubusercontent.com/ermaozi/get_subscribe/main/subscribe/v2ray.yml",
                "https://raw.githubusercontent.com/soroushchehresa/awesome-tracker-list/master/README.md",
                "https://raw.githubusercontent.com/roflmuffin/ProxyList/master/proxies.txt",
                "https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt",
                "https://raw.githubusercontent.com/clarketm/proxy-list/master/proxy-list.txt",
                "https://raw.githubusercontent.com/fate0/proxylist/master/proxy.list"
            ],
            "domain": "free-proxies",
            "rename": "🆓{name}",
            "include": "免费|试用|节点",
            "exclude": "付费|VIP|过期|套餐",
            "push_to": ["MyProxies"],
            "ignorede": false,
            "rate": 1.0,
            "count": 50,
            "secure": false
        },
        {
            "name": "高速节点",
            "enable": true,
            "liveness": true,
            "sub": [
                "https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/Proxy.ini",
                "https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/UnBan.ini",
                "https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/LocalAreaNetwork.ini"
            ],
            "domain": "premium-proxies",
            "rename": "⚡{name}",
            "include": "专线|高速|游戏",
            "exclude": "免费|测试",
            "push_to": ["MyProxies"],
            "ignorede": false,
            "rate": 1.0,
            "count": 30,
            "secure": false
        },
        {
            "name": "测试专用节点",
            "enable": true,
            "liveness": true,
            "sub": [
                "https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/Proxies.ini",
                "https://raw.githubusercontent.com/Pawdroid/Free-servers/main/sub"
            ],
            "domain": "test-proxies",
            "rename": "🧪{name}",
            "push_to": ["MyProxies"],
            "count": 10
        },
        {
            "name": "全球代理节点",
            "enable": true,
            "liveness": true,
            "sub": [
                "https://proxylist.geonode.com/api/proxy-list?limit=500&page=1&sort_by=lastChecked&sort_type=desc",
                "https://raw.githubusercontent.com/proxifly/free-proxy-list/main/proxies/https/data.txt",
                "https://raw.githubusercontent.com/MuRongPIG/Proxy-Master/main/proxy.txt"
            ],
            "domain": "global-proxies",
            "rename": "🌍{name}",
            "include": "US|UK|JP|SG|KR|DE|FR|CA|AU",
            "exclude": "CN|中国",
            "push_to": ["MyProxies"],
            "count": 20
        }
    ],
    "crawl": {
        "enable": true,
        "exclude": "付费|广告|无效|垃圾",
        "threshold": 50,
        "singlelink": true,
        "persist": {
            "subs": "crawled:subs",
            "proxies": "crawled:proxies"
        },
        "config": {
            "rename": "📥采集-{name}",
            "include": "节点|订阅|免费|代理"
        },
        "telegram": {
            "enable": true,
            "pages": 15,
            "exclude": "垃圾|收费|广告",
            "users": {
                "channel": {
                    "include": "vpn|节点|免费|代理",
                    "config": {
                        "rename": "📢TG-{name}"
                    },
                    "push_to": ["MyProxies"]
                }
            }
        },
        "google": {
            "enable": true,
            "exclude": "百度|搜狗|广告",
            "notinurl": ["baidu.com", "sogou.com"],
            "push_to": ["MyProxies"]
        },
        "twitter": {
            "enable": true,
             "users": {
                "username": {
                     "enable": true,
                     "num": 100,
                     "include": "节点|订阅链接|免费",
                      "exclude": "广告|付费|抽奖",
                      "config": {
                          "rename": "🐦TW-{name}",
                          "tag": "TWITTER",
                          "rate": 2.5
                     },
                    "push_to": ["MyProxies"]
                }
            }
        },
        "github": {
            "enable": true,
            "pages": 10,
            "push_to": ["MyProxies"],
            "exclude": "过期|失效|广告|垃圾",
            "spams": ["test-repo", "ad-repo", "spam-repo"]
        }
    },
    "groups": {
        "MyProxies": {
            "emoji": true,
            "list": true,
            "targets": {
                "clash": "MyClashProfile",
                "singbox": "xxx-singbox",
                "v2ray": "xxx-v2ray"
            },
            "regularize": {
                "enable": true,
                "locate": true,
                "bits": 2
            }
        }
    },
    "storage": {
        "engine": "gist",
        "base": "https://api.github.com",
        "domain": "https://gist.github.com",
        "items": {
            "MyClashProfile": {
                "username": "oi4976",
                "gistid": "ba2057fb64e8d1ac97f687910810d551",
